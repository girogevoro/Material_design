<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">?superColorPrimary</item>
        <item name="colorPrimaryVariant">?superColorPrimaryVariant</item>
        <item name="colorOnPrimary">?superColorOnPrimary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">?superColorSecondary</item>
        <item name="colorSecondaryVariant">?superColorSecondaryVariant</item>
        <item name="colorOnSecondary">?superColorOnSecondary</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="chipStyle">@style/AppChip.Choice</item>
        <item name="bottomAppBarStyle">@style/AppBottomAppBar</item>

        <item name="android:fontFamily">@font/super_font</item>
        <item name="fontFamily">@font/super_font</item>

    </style>

    <style name="AppThemeBlue" parent="AppTheme">
        <item name="superColorPrimary">#033571</item>
        <item name="superColorPrimaryVariant">#4577DC</item>
        <item name="superColorOnPrimary">#AFCFEC</item>
        <item name="superColorSecondary">#0DEFDC</item>
        <item name="superColorSecondaryVariant">#087988</item>
        <item name="superColorOnSecondary">#A5C5D3</item>
    </style>

    <style name="AppThemeGreen" parent="AppTheme">
        <item name="superColorPrimary">#075552</item>
        <item name="superColorPrimaryVariant">#45f7DC</item>
        <item name="superColorOnPrimary">#AFfFEC</item>
        <item name="superColorSecondary">#0DEfDC</item>
        <item name="superColorSecondaryVariant">#0f7988</item>
        <item name="superColorOnSecondary">#Aff5D3</item>
    </style>

    <style name="AppThemeYellow" parent="AppTheme">
        <item name="superColorPrimary">#4C5104</item>
        <item name="superColorPrimaryVariant">#8A8022</item>
        <item name="superColorOnPrimary">#D1D6A1</item>
        <item name="superColorSecondary">#0DEfDC</item>
        <item name="superColorSecondaryVariant">#0f7988</item>
        <item name="superColorOnSecondary">#Aff5D3</item>
    </style>




</resources>